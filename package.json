{
  "name": "fe-vue2-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "uat": "vue-cli-service build --mode uat",
    "release": "vue-cli-service build",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit --skipLibCheck",
    "lint": "vue-cli-service lint",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.4",
    "axios": "^0.21.1",
    "co": "4",
    "core-js": "^3.20.3",
    "ts-deepmerge": "^2.0.1",
    "ts-essentials": "^9.1.2",
    "vant": "2",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.10",
    "@commitlint/cli": "^15.0.0",
    "@types/node": "^17.0.10",
    "@types/path-to-regexp": "^1.7.0",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.x.x",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.2",
    "less-loader": "^7",
    "lint-staged": "^11.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "10.x",
    "serve": "^13.0.2",
    "svg-sprite-loader": "^6.0.11",
    "ts-gear": "^4.3.0",
    "typescript": "~4.5.4",
    "vite": "^2.7.13",
    "vue-cli-plugin-mock": "~1.0.3",
    "vue-template-babel-compiler": "^1.1.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4",
    "webpack-aliyun-oss": "^0.5.1",
    "yorkie-pnpm": "^2.0.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
