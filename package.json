{
  "name": "fe-vue2-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "uat": "vue-cli-service build --mode uat",
    "release": "vue-cli-service build",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit --skipLibCheck",
    "lint": "vue-cli-service lint",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/"
  },
  "dependencies": {
    "@vue/composition-api": "^1.3.0",
    "axios": "^0.24.0",
    "core-js": "^3.6.5",
    "ts-deepmerge": "^1.1.0",
    "ts-essentials": "^9.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "eslint": "7.x",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.26.5",
    "sass-loader": "10.x",
    "serve": "^12.0.1",
    "svg-sprite-loader": "^6.0.11",
    "ts-gear": "^4.1.0",
    "typescript": "~4.4.4",
    "vue-cli-plugin-mock": "~1.0.3",
    "vue-template-babel-compiler": "^1.0.6-0",
    "vue-template-compiler": "^2.6.11",
    "webpack-aliyun-oss": "^0.3.9"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
