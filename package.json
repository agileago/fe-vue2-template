{
  "name": "fe-vue2-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --no-module",
    "develop": "pnpm run build --mode develop",
    "release": "pnpm run build --mode release",
    "master": "pnpm run build --mode master",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit",
    "lint": "vue-cli-service lint",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.24.1",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.2.0",
    "vue": "^2.7.8",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/path-to-regexp": "^1.7.0",
    "@types/prettier": "^2.7.0",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "serve": "^14.0.1",
    "simple-git-hooks": "^2.8.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.1.8",
    "ts-gear": "^4.9.0",
    "typescript": "~4.7.4",
    "vite": "^3.0.7",
    "vue-cli-plugin-mock": "~1.0.3",
    "vue-template-compiler": "^2.7.8",
    "webpack": "^5.74.0",
    "webpack-aliyun-oss": "^0.5.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
