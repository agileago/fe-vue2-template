{
  "name": "fe-vue2-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --no-module",
    "preview": "serve -s dist",
    "type:check": "tsc --noEmit",
    "lint": "vue-cli-service lint",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "core-js": "^3.26.0",
    "path-to-regexp": "^6.2.1",
    "ts-essentials": "^9.3.0",
    "vue": "^2.7.8",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/path-to-regexp": "^1.7.0",
    "@types/prettier": "^2.7.1",
    "@types/swagger-schema-official": "^2.0.22",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-typescript": "^11.0.2",
    "babel-loader": "^9.0.0",
    "babel-plugin-import": "^1.13.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "simple-git-hooks": "^2.8.1",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.2.1",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "ts-gear": "^4.11.4",
    "typescript": "~4.8.4",
    "vite": "^3.2.1",
    "vue-cli-plugin-mock": "~1.0.3",
    "vue-template-compiler": "^2.7.13",
    "webpack": "^5.74.0",
    "webpack-aliyun-oss": "^0.5.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint"
    ],
    "*.{css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
